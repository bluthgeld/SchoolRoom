

<h1>Create A New Educator Profile</h1>

<%if @educator.errors%>
<%@educator.errors.full_messages.each do |message|%>
  <div class="error">
    <%=message%>
  </div>
  <%end%>
<%end%>




<%= form_for @educator do |f|%>
<div class="wrapper">
  <div class="box_l">
    <%= f.label :first_name %>
  </div>
  <div class="box_r">
    <%= f.text_field :first_name%>
  </div>
</div>
<div class="wrapper">
  <div class="box_l">
    <%= f.label :last_name %>
  </div>
  <div class="box_r">
    <%= f.text_field :last_name%>
  </div>
</div>
<div class="wrapper">
  <div class="box_l">
    <%= f.label :email %>
  </div>
  <div class="box_r">
    <%= f.text_field :email%>
  </div>
</div>
<div class="wrapper">
  <div class="box_l">
    <%= f.label :phone %>
  </div>
  <div class="box_r">
    <%= f.text_field :phone%>
  </div>
</div>

<%= f.fields_for :users do |u|%>

<div class="wrapper">
  <div class="box_l">
    <%= u.label :username %>
  </div>
  <div class="box_r">
    <%= u.text_field :username %>
  </div>
</div>
<div class="wrapper">
  <div class="box_l">
    <%= u.label :password %>
  </div>
  <div class="box_r">
    <%= u.password_field :password%>
  </div>
</div>

<%end%>

<%= f.submit %>

<% end %>
