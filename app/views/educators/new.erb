<h2>Create Username and Password</h2>

<div>
<%= form_for @educator do |f|%>
<%= f.label :first_name %>
<%= f.text_field :first_name%>
<%= f.label :last_name %>
<%= f.text_field :last_name %>
<%= f.label :email %>
<%= f.text_field :email %>
<%= f.label :phone %>
<%= f.text_field :phone%></div>


<div>
<%= f.fields_for :users do |u|%>
<%= u.label :username %>
<%= u.text_field :username%>
<%= u.label :password%>
<%= u.password_field :password%>

<%end%>
<%= f.submit %></div>
<%end%>
